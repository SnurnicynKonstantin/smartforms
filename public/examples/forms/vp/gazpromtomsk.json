          },
          {
            "name": "июль",
            "value": "07"
          },
          {
            "name": "август",
            "value": "08"
          },
          {
            "name": "сентябрь",
            "value": "09"
          },
          {
            "name": "октябрь",
            "value": "10"
          },
          {
            "name": "ноябрь",
            "value": "11"
          },
          {
            "name": "декабрь",
            "value": "12"
          }
