      },
      "Сумма редактируемая": {
        "block": "text",
        "placeholder": "0",
        "label": "Сумма",
        "labelWidth": 3,
        "inputWidth": "200px",
        "textAlign": "right",
        "value": 0,
        "rightMark": "<span class=\"glyphicon glyphicon-ruble\" aria-hidden=\"true\" style=\"font-size: 11px\"><\/span>"
      },
      "div": {
        "block": "divider"
      },
      "Газ метка": {
        "block": "label",
        "labelText": "Газ"
      },
      "Газ": {
        "block": "text",
        "value": 0,
        "isSummable": true,
        "textAlign": "center"
      },
      "Предыдущие показания": {
        "block": "comment",
        "text": "Предыдущие показания прибора учета: 2708",
        "fontColor": "#252525",
        "fontSize": "13px"
      },
      "Сумма": {
        "block": "static",
        "label": "Сумма",
        "labelWidth": 3,
        "value": 0,
        "rightMark": "<span class=\"glyphicon glyphicon-ruble\" aria-hidden=\"true\"><\/span>",
        "summarize": [
          "Сумма редактируемая"
        ],
        "validationRules": [
          {
            "rule": "1 <= Сумма && Сумма <= 15000",
            "errorMessage": "Сумма платежа не может быть меньше 1 руб. и больше 15 000 руб.",
            "isGlobalError": true
