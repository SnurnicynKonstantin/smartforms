    "schema": [
      "accountId",
      {
        "block": "fieldset",
        "label": "Плательщик",
        "labelWidth": 3,
        "items": [
          "lastname",
          "firstname",
          "patronym"
        ]
      },
      "address",
      "div",
      {
        "name": "fieldset1",
        "block": "fieldset",
        "label": "Период оплаты",
        "labelWidth": 3,
        "layout": [
          {
            "count": 2,
            "width": [
              4,
              4
            ]
          }
        ],
        "items": [
          "month",
          "year"
        ]
